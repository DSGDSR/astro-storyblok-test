---
import Testimonial from './Testimonial.astro';
import Hero from './Hero.astro';
import Features from './Features.astro';
import Cta from './Cta.astro';
import Container from './Container.astro';
import Button from './Button.astro';
import DescriptionList from './DescriptionList.astro';

const { children } = Astro.props
---

{children?.map(blok => {
	switch(blok.component) {
		case 'button':
			return <Button blok={blok} />
		case 'testimonial':
			return <Testimonial blok={blok} />
		case 'hero':
			return <Hero blok={blok} />
		case 'features':
			return <Features blok={blok} />
		case 'cta':
			return <Cta blok={blok} />
		case 'cta':
			return <Cta blok={blok} />
		case 'container':
			return <Container blok={blok} />
		case 'description-list':
			return <DescriptionList blok={blok} />
	}
})}